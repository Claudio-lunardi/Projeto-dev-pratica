@model MaisSaude.Models.tUser.tTitular.tTitularRetorno

@{
    ViewData["Title"] = "Edit";
}

<div class="container">
    <div class="row justify-content-center ">
        <div class="col-md-7">
            <div class="card">
                <div class="card-body">
                    <form asp-action="Edit">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input asp-for="tUser.ID" class="form-control" type="hidden" />
                        <input asp-for="tUserData.ID" class="form-control" type="hidden" />
                        <input asp-for="tUser.DataCriacao" class="form-control" type="hidden" />
                        <input asp-for="tUserData.DateUpdate" class="form-control" type="hidden" />
                        <input id="senha1" type="password" asp-for="tUser.Senha" type="hidden" class="form-control" value="____________"/>
                        <input id="senha2" type="password" asp-for="tUser.Senha" type="hidden" class="form-control" value="____________"/>
                        <input class="form-check-input" asp-for="tUser.Ativo" type="hidden" />

                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group mb-3">
                                    <label asp-for="tUser.Nome" class="control-label"></label>
                                    <input asp-for="tUser.Nome" class="form-control" placeholder="João..." />
                                    <span asp-validation-for="tUser.Nome" class="text-danger"></span>
                                </div>
                            </div>

                        </div>

                        <div class="row">
                            <div class="col-md-6 mb-3">
                                <div class="form-group">
                                    <label asp-for="tUser.Email" class="control-label"></label>
                                    <input type="email" asp-for="tUser.Email" class="form-control" />
                                    <span asp-validation-for="tUser.Email" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-md-6 mb-3">
                                <div class="form-group">
                                    <label asp-for="tUserData.CPF" class="control-label"></label>
                                    <input asp-for="tUserData.CPF" class="form-control maskCPF" placeholder="___.___.___-__" />
                                    <span asp-validation-for="tUserData.CPF" class="text-danger"></span>
                                </div>
                            </div>
                        </div>

                        <hr class="mt-5 mb-5" />

                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label asp-for="tUserData.Telefone" class="control-label"></label>
                                    <input asp-for="tUserData.Telefone" class="form-control maskTelefone" placeholder="(__) ____-____" />
                                    <span asp-validation-for="tUserData.Telefone" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label asp-for="tUserData.Celular" class="control-label"></label>
                                    <input asp-for="tUserData.Celular" class="form-control maskCelular" placeholder="(__) _____-____" />
                                    <span asp-validation-for="tUserData.Celular" class="text-danger"></span>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label asp-for="tUserData.CEP" class="control-label"></label>
                            <input id="CEP" asp-for="tUserData.CEP" class="form-control maskCEP" placeholder="_____-___ " />
                            <span asp-validation-for="tUserData.CEP" class="text-danger"></span>
                        </div>

                        <div class="form-group">
                            <label asp-for="tUserData.Cidade" class="control-label"></label>
                            <input id="Cidade" asp-for="tUserData.Cidade" class="form-control" readonly />
                            <span asp-validation-for="tUserData.Cidade" class="text-danger"></span>
                        </div>

                        <div class="form-group">
                            <label asp-for="tUserData.Bairro" class="control-label"></label>
                            <input id="Bairro" asp-for="tUserData.Bairro" class="form-control" readonly />
                            <span asp-validation-for="tUserData.Bairro" class="text-danger"></span>
                        </div>

                        <div class="form-group">
                            <label asp-for="tUserData.Logradouro" class="control-label"></label>
                            <input id="logradouro" asp-for="tUserData.Logradouro" class="form-control" readonly />
                            <span asp-validation-for="tUserData.Logradouro" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="tUserData.UF" class="control-label"></label>
                            <input id="UF" asp-for="tUserData.UF" class="form-control" readonly />
                            <span asp-validation-for="tUserData.UF" class="text-danger"></span>
                        </div>

                        <div class="form-group">
                            <label asp-for="tUserData.Numero" class="control-label"></label>
                            <input asp-for="tUserData.Numero" class="form-control" />
                            <span asp-validation-for="tUserData.Numero" class="text-danger"></span>
                        </div>

                        <div class="form-group">
                            <label asp-for="tUserData.Complemento" class="control-label"></label>
                            <input asp-for="tUserData.Complemento" class="form-control" />
                            <span asp-validation-for="tUserData.Complemento" class="text-danger"></span>
                        </div>

                        <div class="card-footer mt-4">

                            <div class="text-center mt-2">
                                <div class="row">
                                    <input type="submit" value="Salvar" class="btn btn-primary" />
                                </div>
                            </div>

                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

<script type="text/javascript">

   

</script>